{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "appserver",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register/transpile-only"
      ],
      "args": [
        "src/appserver/main.ts"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
